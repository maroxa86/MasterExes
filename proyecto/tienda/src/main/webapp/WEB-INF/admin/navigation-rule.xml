<?xml version="1.0"?>
<faces-config version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
	<application />
	
	<navigation-rule>
		<from-view-id>/admin.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>inventarioAdmin</from-outcome>
			<to-view-id>/inventarioAlmacenAdmin.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>crearUsuario</from-outcome>
			<to-view-id>/crearUsuario.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/crearUsuario.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>inventarioAdmin</from-outcome>
			<to-view-id>/inventarioAlmacenAdmin.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>perfil</from-outcome>
			<to-view-id>/admin.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>crearUsuario</from-outcome>
			<to-view-id>/crearUsuario.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
</faces-config>
