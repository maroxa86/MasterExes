<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
	<navigation-rule>
		<from-view-id>/login.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>admin</from-outcome>
			<to-view-id>/admin.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>comercial</from-outcome>
			<to-view-id>/comercial.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>mozoAlmacen</from-outcome>
			<to-view-id>/mozoAlmacen.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/admin.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>inventarioAdmin</from-outcome>
			<to-view-id>/inventarioAlmacenAdmin.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>crearUsuario</from-outcome>
			<to-view-id>/crearUsuario.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/crearUsuario.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>inventarioAdmin</from-outcome>
			<to-view-id>/inventarioAlmacenAdmin.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>perfil</from-outcome>
			<to-view-id>/admin.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>crearUsuario</from-outcome>
			<to-view-id>/crearUsuario.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/mozoAlmacen.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>perfil</from-outcome>
			<to-view-id>/mozoAlmacen.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>inventario</from-outcome>
			<to-view-id>/inventarioAlmacen.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/inventarioAlmacen.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>perfil</from-outcome>
			<to-view-id>/mozoAlmacen.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>inventario</from-outcome>
			<to-view-id>/inventarioAlmacen.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>newProducto</from-outcome>
			<to-view-id>/newProducto.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>editarProducto</from-outcome>
			<to-view-id>/editProducto.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/newProducto.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>inventario</from-outcome>
			<to-view-id>/inventarioAlmacen.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/editProducto.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>inventario</from-outcome>
			<to-view-id>/inventarioAlmacen.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/comercial.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>clientes</from-outcome>
			<to-view-id>/clientes.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/clientes.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>newCliente</from-outcome>
			<to-view-id>/newCliente.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>perfil</from-outcome>
			<to-view-id>/comercial.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/newCliente.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>clientes</from-outcome>
			<to-view-id>/clientes.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>perfil</from-outcome>
			<to-view-id>/comercial.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<application>
		<el-resolver>org.springframework.web.jsf.el.SpringBeanFacesELResolver</el-resolver>
	</application>
	
</faces-config>