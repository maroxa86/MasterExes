<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    
    <navigation-rule>
        <from-view-id>/secured/admin/admin.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>inventario</from-outcome>
            <to-view-id>/secured/admin/inventario.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>crearUsuario</from-outcome>
            <to-view-id>/secured/admin/crearUsuario.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>bandejaDeEntrada</from-outcome>
            <to-view-id>/secured/admin/bandejaDeEntrada.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>/secured/admin/inventario.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>perfil</from-outcome>
            <to-view-id>/secured/admin/admin.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>crearUsuario</from-outcome>
            <to-view-id>/secured/admin/crearUsuario.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>/secured/admin/crearUsuario.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>perfil</from-outcome>
            <to-view-id>/secured/admin/admin.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>inventario</from-outcome>
            <to-view-id>/secured/admin/inventario.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>/secured/admin/bandejaDeEntrada.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>inventario</from-outcome>
            <to-view-id>/secured/admin/inventario.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>crearUsuario</from-outcome>
            <to-view-id>/secured/admin/crearUsuario.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>perfil</from-outcome>
            <to-view-id>/secured/admin/admin.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>leerMensaje</from-outcome>
            <to-view-id>/secured/admin/mensaje.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>/secured/admin/mensaje.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>inventario</from-outcome>
            <to-view-id>/secured/admin/inventario.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>crearUsuario</from-outcome>
            <to-view-id>/secured/admin/crearUsuario.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>perfil</from-outcome>
            <to-view-id>/secured/admin/admin.xhtml</to-view-id>
        </navigation-case>  
    </navigation-rule>
    
</faces-config>
